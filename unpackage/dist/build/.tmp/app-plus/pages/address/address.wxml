<view class="content b-t"><block wx:for="{{AddressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['checkAddress',['$0'],[[['AddressList','',index,'id']]]]]]]}}" class="list b-b" bindtap="__e"><view class="wrapper"><view class="address-box"><block wx:if="{{item.default==1}}"><text class="tag">默认</text></block><text class="address">{{item.province+" "+item.city+" "+item.district+" "+item.details}}</text></view><view class="u-box"><text class="name">{{item.consignee}}</text><text class="mobile">{{item.phone}}</text></view></view><text data-event-opts="{{[['tap',[['del',['$0'],[[['AddressList','',index,'id']]]]]]]}}" class="yticon  icon-iconfontshanchu1" style="color:#555555;font-size:34rpx;" catchtap="__e"></text><text data-event-opts="{{[['tap',[['addAddress',['edit','$0'],[[['AddressList','',index,'id']]]]]]]}}" class="yticon icon-bianji" catchtap="__e"></text></view></block><button data-event-opts="{{[['tap',[['addAddress',['add']]]]]}}" class="add-btn" bindtap="__e">新增地址</button></view>